ame":"kid","original":{"line":44,"column":20},"generated":{"line":1,"column":735}},{"source":"kiddos.js","name":"annualCostValues","original":{"line":46,"column":8},"generated":{"line":1,"column":738}},{"source":"kiddos.js","name":"push","original":{"line":46,"column":25},"generated":{"line":1,"column":740}},{"source":"kiddos.js","name":"kid","original":{"line":46,"column":30},"generated":{"line":1,"column":745}},{"source":"kiddos.js","name":"yearlyCost","original":{"line":46,"column":34},"generated":{"line":1,"column":747}},{"source":"kiddos.js","name":"year","original":{"line":46,"column":45},"generated":{"line":1,"column":758}},{"source":"kiddos.js","name":"monthlyCost","original":{"line":46,"column":51},"generated":{"line":1,"column":760}},{"source":"kiddos.js","name":"annualCostValues","original":{"line":49,"column":23},"generated":{"line":1,"column":766}},{"source":"kiddos.js","name":"reduce","original":{"line":49,"column":40},"generated":{"line":1,"column":768}},{"source":"kiddos.js","original":{"line":49,"column":47},"generated":{"line":1,"column":775}},{"source":"kiddos.js","name":"previousValue","original":{"line":49,"column":48},"generated":{"line":1,"column":784}},{"source":"kiddos.js","name":"currentValue","original":{"line":49,"column":63},"generated":{"line":1,"column":786}},{"source":"kiddos.js","name":"previousValue","original":{"line":50,"column":15},"generated":{"line":1,"column":789}},{"source":"kiddos.js","name":"previousValue","original":{"line":50,"column":15},"generated":{"line":1,"column":796}},{"source":"kiddos.js","name":"currentValue","original":{"line":50,"column":31},"generated":{"line":1,"column":798}},{"source":"kiddos.js","name":"kidExports","original":{"line":56,"column":6},"generated":{"line":1,"column":803}},{"source":"kiddos.js","original":{"line":56,"column":19},"generated":{"line":1,"column":805}},{"source":"kiddos.js","name":"returnAnnualKidCost","original":{"line":56,"column":20},"generated":{"line":1,"column":806}},{"source":"kiddos.js","name":"returnAnnualKidCost","original":{"line":56,"column":20},"generated":{"line":1,"column":826}},{"source":"kiddos.js","name":"kidCreator","original":{"line":56,"column":41},"generated":{"line":1,"column":828}},{"source":"kiddos.js","name":"kidCreator","original":{"line":56,"column":41},"generated":{"line":1,"column":839}},{"source":"kiddos.js","name":"module","original":{"line":57,"column":0},"generated":{"line":1,"column":842}},{"source":"kiddos.js","name":"exports","original":{"line":57,"column":7},"generated":{"line":1,"column":849}},{"source":"kiddos.js","name":"kidExports","original":{"line":57,"column":17},"generated":{"line":1,"column":857}}],"sources":{"kiddos.js":"//Kid creator takes all the required variables related to the cost of a kid, and saves them as an object.  This is a factory function for kid objects\r\n//Really only need a name and year born, default values are reasonable\r\n//Important method is yearlyCost\r\nconsole.log(`Time kid page top at ${new Date().getSeconds()} and ${new Date().getMilliseconds()}`);\r\n\r\nconst taxCredit = 1500;\r\nconst kidCreator = (_name='Kid', _yearBorn=2020, _dayCareCost=0, _collegeCost=30000) =>{\r\n    return {\r\n        _name,\r\n        _yearBorn,\r\n        _dayCareCost,\r\n        _collegeCost,\r\n        //With above information, can calculate estimated cost for this kid for a given year\r\n        yearlyCost(seekingYear, monthlyCost){\r\n            let cost = 0;\r\n            //if seeking year is before birth year, cost is 0\r\n            if (seekingYear<this._yearBorn){\r\n                cost =0;\r\n            //if seeking year occurs between age 0 and 4, enter this section\r\n            }else if (seekingYear-this._yearBorn<4){\r\n                //cost is regular monthly cost + daycare cost (which can be zero)\r\n                cost = (monthlyCost + this._dayCareCost)*12-taxCredit;\r\n            //This section is for kids aged 4-18\r\n            }else if (seekingYear-this._yearBorn<=18){\r\n                cost = monthlyCost*12-taxCredit;\r\n            //Seeking year falls within college years\r\n            }else if (seekingYear-this._yearBorn>18 && seekingYear-this._yearBorn<23){\r\n                cost = this._collegeCost-taxCredit;\r\n            //Kid has left the nest, no cost\r\n            }else if (seekingYear-this._yearBorn>22){\r\n                cost = 0;\r\n            }\r\n            return cost;\r\n        }  \r\n    }\r\n}\r\n\r\n//This function takes a year as a parameter and returns the annual cost of having kids for that year.  Will add up costs of multiple kids at the same time\r\nconst returnAnnualKidCost = (year, kidList, monthlyCost) =>{\r\n    if(kidList.length==0){\r\n        return 0;\r\n    }\r\n    const annualCostValues = [];\r\n    kidList.forEach(kid => {\r\n        //for each kid, add the cost for the given year to an array\r\n        annualCostValues.push(kid.yearlyCost(year, monthlyCost));\r\n    })\r\n    //then reduce above array to sum up kid cost values into one total cost number\r\n    const annualCost = annualCostValues.reduce((previousValue, currentValue)=>{\r\n        return previousValue + currentValue;\r\n    })\r\n    return annualCost;\r\n}\r\n\r\n//export annual kid cost calculator to be used in overall life yearly calculations\r\nconst kidExports = {returnAnnualKidCost, kidCreator}\r\nmodule.exports = kidExports;"},"lineCount":null}},"error":null,"hash":"5f76aaf1a545f5624bba9c8f8160be04","cacheData":{"env":{}}}