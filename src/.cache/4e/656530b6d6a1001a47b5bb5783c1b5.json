ed":{"line":62,"column":4},"source":"kiddos.js","original":{"line":56,"column":6}},{"generated":{"line":62,"column":14},"source":"kiddos.js","original":{"line":56,"column":16}},{"generated":{"line":62,"column":17},"source":"kiddos.js","original":{"line":56,"column":19}},{"name":"returnAnnualKidCost","generated":{"line":63,"column":0},"source":"kiddos.js","original":{"line":56,"column":20}},{"name":"returnAnnualKidCost","generated":{"line":63,"column":2},"source":"kiddos.js","original":{"line":56,"column":20}},{"generated":{"line":63,"column":21},"source":"kiddos.js","original":{"line":56,"column":39}},{"name":"returnAnnualKidCost","generated":{"line":63,"column":23},"source":"kiddos.js","original":{"line":56,"column":20}},{"generated":{"line":63,"column":42},"source":"kiddos.js","original":{"line":56,"column":19}},{"name":"kidCreator","generated":{"line":64,"column":0},"source":"kiddos.js","original":{"line":56,"column":41}},{"name":"kidCreator","generated":{"line":64,"column":2},"source":"kiddos.js","original":{"line":56,"column":41}},{"generated":{"line":64,"column":12},"source":"kiddos.js","original":{"line":56,"column":51}},{"name":"kidCreator","generated":{"line":64,"column":14},"source":"kiddos.js","original":{"line":56,"column":41}},{"generated":{"line":65,"column":0},"source":"kiddos.js","original":{"line":56,"column":19}},{"generated":{"line":65,"column":1},"source":"kiddos.js","original":{"line":56,"column":0}},{"name":"module","generated":{"line":66,"column":0},"source":"kiddos.js","original":{"line":57,"column":0}},{"generated":{"line":66,"column":6},"source":"kiddos.js","original":{"line":57,"column":6}},{"name":"exports","generated":{"line":66,"column":7},"source":"kiddos.js","original":{"line":57,"column":7}},{"generated":{"line":66,"column":14},"source":"kiddos.js","original":{"line":57,"column":0}},{"name":"kidExports","generated":{"line":66,"column":17},"source":"kiddos.js","original":{"line":57,"column":17}},{"generated":{"line":66,"column":27},"source":"kiddos.js","original":{"line":57,"column":0}}],"sources":{"kiddos.js":"//Kid creator takes all the required variables related to the cost of a kid, and saves them as an object.  This is a factory function for kid objects\r\n//Really only need a name and year born, default values are reasonable\r\n//Important method is yearlyCost\r\nconsole.log(`Time kid page top at ${new Date().getSeconds()} and ${new Date().getMilliseconds()}`);\r\n\r\nconst taxCredit = 1500;\r\nconst kidCreator = (_name='Kid', _yearBorn=2020, _dayCareCost=0, _collegeCost=30000) =>{\r\n    return {\r\n        _name,\r\n        _yearBorn,\r\n        _dayCareCost,\r\n        _collegeCost,\r\n        //With above information, can calculate estimated cost for this kid for a given year\r\n        yearlyCost(seekingYear, monthlyCost){\r\n            let cost = 0;\r\n            //if seeking year is before birth year, cost is 0\r\n            if (seekingYear<this._yearBorn){\r\n                cost =0;\r\n            //if seeking year occurs between age 0 and 4, enter this section\r\n            }else if (seekingYear-this._yearBorn<4){\r\n                //cost is regular monthly cost + daycare cost (which can be zero)\r\n                cost = (monthlyCost + this._dayCareCost)*12-taxCredit;\r\n            //This section is for kids aged 4-18\r\n            }else if (seekingYear-this._yearBorn<=18){\r\n                cost = monthlyCost*12-taxCredit;\r\n            //Seeking year falls within college years\r\n            }else if (seekingYear-this._yearBorn>18 && seekingYear-this._yearBorn<23){\r\n                cost = this._collegeCost-taxCredit;\r\n            //Kid has left the nest, no cost\r\n            }else if (seekingYear-this._yearBorn>22){\r\n                cost = 0;\r\n            }\r\n            return cost;\r\n        }  \r\n    }\r\n}\r\n\r\n//This function takes a year as a parameter and returns the annual cost of having kids for that year.  Will add up costs of multiple kids at the same time\r\nconst returnAnnualKidCost = (year, kidList, monthlyCost) =>{\r\n    if(kidList.length==0){\r\n        return 0;\r\n    }\r\n    const annualCostValues = [];\r\n    kidList.forEach(kid => {\r\n        //for each kid, add the cost for the given year to an array\r\n        annualCostValues.push(kid.yearlyCost(year, monthlyCost));\r\n    })\r\n    //then reduce above array to sum up kid cost values into one total cost number\r\n    const annualCost = annualCostValues.reduce((previousValue, currentValue)=>{\r\n        return previousValue + currentValue;\r\n    })\r\n    return annualCost;\r\n}\r\n\r\n//export annual kid cost calculator to be used in overall life yearly calculations\r\nconst kidExports = {returnAnnualKidCost, kidCreator}\r\nmodule.exports = kidExports;"},"lineCount":null}},"error":null,"hash":"4dbff451747fff7c2a2f1470f7c89671","cacheData":{"env":{}}}